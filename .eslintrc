{
  "root": true,
  "env":  {
    "browser":  true,
    "jquery":   true,
    "commonjs": true
  },
  "parserOptions":{
    "ecmaVersion":  8,
    "sourceType":   "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules":{
    "no-cond-assign":1,
    "no-console":1,
    "no-constant-condition":1,
    "no-control-regex":1,
    "no-debugger":1,
    "no-dupe-args":1,
    "no-dupe-keys":1,
    "no-duplicate-case":1,
    "no-empty":1,
    "no-empty-character-class":1,
    "no-ex-assign":1,
    "no-extra-boolean-cast":1,
    "no-extra-parens":0,
    "no-extra-semi":1,
    "no-func-assign":1,
    "no-inner-declarations":[
      1,
      "functions"
    ],
    "no-invalid-regexp":1,
    "no-irregular-whitespace":1,
    "no-negated-in-lhs":1,
    "no-obj-calls":1,
    "no-regex-spaces":1,
    "no-sparse-arrays":1,
    "no-unexpected-multiline":1,
    "no-unreachable":1,
    "use-isnan":1,
    "valid-jsdoc":1,
    "valid-typeof":1,
    "accessor-pairs":0,
    "array-callback-return":1,
    "block-scoped-var":0,
    "complexity":[
      1,
      10
    ],
    "default-case":1,
    "dot-location":[
      1,
      "property"
    ],
    "guard-for-in":1,
    "no-alert":1,
    "no-caller":1,
    "no-div-regex":0,
    "no-else-return":1,
    "no-eq-null":1,
    "no-eval":1,
    "no-extend-native":1,
    "no-extra-bind":1,
    "no-extra-label":1,
    "no-fallthrough":1,
    "no-floating-decimal":1,
    "no-implicit-coercion":[
      1,
      {
        "boolean":false,
        "number":false,
        "string":false
      }
    ],
    "no-implied-eval":1,
    "no-invalid-this":1,
    "no-iterator":1,
    "no-labels":1,
    "no-lone-blocks":1,
    "no-loop-func":1,
    "no-multi-str":1,
    "no-native-reassign":1,
    "no-new":1,
    "no-new-func":1,
    "no-new-wrappers":1,
    "no-octal":1,
    "no-octal-escape":1,
    "no-param-reassign":[
      1,
      {
        "props":false
      }
    ],
    "no-process-env":1,
    "no-proto":1,
    "no-redeclare":1,
    "no-return-assign":1,
    "no-script-url":1,
    "no-self-assign":1,
    "no-self-compare":1,
    "no-sequences":1,
    "no-throw-literal":1,
    "no-unmodified-loop-condition":1,
    "no-unused-labels":1,
    "no-useless-call":0,
    "no-useless-concat":1,
    "no-void":1,
    "no-warning-comments":[
      0,
      {
        "terms":[
          "todo",
          "fixme"
        ],
        "location":"start"
      }
    ],
    "no-with":1,
    "radix":1,
    "vars-on-top":0,
    "wrap-iife":[
      0,
      "inside"
    ],
    "yoda":[
      1,
      "never"
    ],
    "init-declarations":0,
    "no-catch-shadow":1,
    "no-delete-var":1,
    "no-label-var":1,
    "no-shadow":1,
    "no-shadow-restricted-names":1,
    "no-undef-init":1,
    "no-undef":1,
    "no-undefined":0,
    "no-unused-vars":[
      1,
      {
        "vars":"all",
        "args":"after-used",
        "argsIgnorePattern":"^_"
      }
    ],
    "no-use-before-define":1,
    "callback-return":0,
    "global-require":1,
    "handle-callback-err":1,
    "no-mixed-requires":[
      1,
      false
    ],
    "no-new-require":1,
    "no-path-concat":1,
    "no-process-exit":0,
    "no-sync":1,
    "array-bracket-spacing":[
      1,
      "never"
    ],
    "block-spacing":1,
    "camelcase":0,
    "comma-spacing":[
      1,
      {
        "before":false,
        "after":true
      }
    ],
    "comma-style":[
      1,
      "first",
      {
        "exceptions":{
          "ArrayExpression":true,
          "ObjectExpression":true
        }
      }
    ],
    "computed-property-spacing":[
      1,
      "never"
    ],
    "consistent-this":[
      1,
      "thisArg"
    ],
    "eol-last":1,
    "func-names":0,
    "func-style":1,
    "indent":[
      1,
      2,
      {
        "SwitchCase":2
      }
    ],
    "jsx-quotes":1,
    "key-spacing":[
      1,
      {
        "singleLine":{
          "beforeColon":false,
          "afterColon":true
        },
        "multiLine":{
          "beforeColon":false,
          "afterColon":true,
          "align":"value"
        }
      }
    ],
    "keyword-spacing":[
      1,
      {
        "before":true,
        "after":true
      }
    ],
    "linebreak-style":1,
    "lines-around-comment":1,
    "max-depth":[
      1,
      {
        "maximum":5
      }
    ],
    "max-len":[
      1,
      {
        "code":120,
        "tabWidth":1,
        "ignoreUrls":true
      }
    ],
    "max-nested-callbacks":[
      1,
      {
        "maximum":4
      }
    ],
    "max-params":[
      1,
      {
        "maximum":4
      }
    ],
    "max-statements":[
      1,
      {
        "maximum":100
      }
    ],
    "new-cap":[
      1,
      {
        "newIsCap":true,
        "capIsNew":false
      }
    ],
    "new-parens":1,
    "newline-per-chained-call":[
      0,
      {
        "ignoreChainWithDepth":3
      }
    ],
    "no-array-constructor":1,
    "no-inline-comments":0,
    "no-lonely-if":1,
    "no-mixed-spaces-and-tabs":[
      1,
      false
    ],
    "no-multiple-empty-lines":[
      1,
      {
        "max":3
      }
    ],
    "no-negated-condition":1,
    "no-nested-ternary":1,
    "no-new-object":1,
    "no-spaced-func":1,
    "no-ternary":0,
    "no-trailing-spaces":1,
    "no-unneeded-ternary":1,
    "no-whitespace-before-property":1,
    "object-curly-spacing":[
      1,
      "always",
      {
        "objectsInObjects":false,
        "arraysInObjects":false
      }
    ],
    "one-var":0,
    "operator-assignment":0,
    "operator-linebreak":[
      1,
      "before"
    ],
    "padded-blocks":[
      1,
      "never"
    ],
    "quote-props":[
      1,
      "consistent-as-needed"
    ],
    "quotes":[
      1,
      "single",
      "avoid-escape"
    ],
    "id-match":0,
    "sort-vars":0,
    "space-before-blocks":[
      1,
      "always"
    ],
    "space-before-function-paren":[
      1,
      "always"
    ],
    "space-in-parens":[
      1,
      "never"
    ],
    "space-infix-ops":1,
    "space-unary-ops":1,
    "spaced-comment":[
      1,
      "always"
    ],
    "wrap-regex":0
  },
  "globals": {
    "WMATIC": true,
    "BigNumber": true,
    "Promise": true,
    "before": true,
    "beforeEach": true,
    "describe": true,
    "ethers": true,
    "expect": true,
    "global": true,
    "hre": true,
    "it": true,
    "network": true,
    "owner": true,
    "web3": true,
  }
}
